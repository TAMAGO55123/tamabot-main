import discord
from discord.ext import commands
from discord import app_commands

class NameCog(commands.Cog):
    def __init__(self, bot):
        self.bot = bot
    
    @commands.Cog.listener()
    async def on_ready(self):
        print("Cog ready!")
	

async def setup(bot):
    await bot.add_cog(NameCog(bot))